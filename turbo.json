{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "rm-build-dir": {
      "cache": false,
      "dependsOn": [
        "^rm-build-dir"
      ]
    },
    "test": {
      "cache": false,
      "dependsOn": ["build"]
    },
    "lint": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "rm-build-dir",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "move-build": {
      "dependsOn": ["^move-build"],
      "persistent": true,
      "cache": false
    }
  }
}